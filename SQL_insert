create database jojo  COLLATE Chinese_Taiwan_Stroke_90_CI_AS_SC ;

create database jojo
use jojo
-----------------------------------------------------------
CREATE Table WebRole(
wrid int not null primary key,
wrName NVARCHAR(10)
);
----------------------------------------------------------
CREATE TABLE Citys (
   addCityid INT NOT NULL PRIMARY KEY,
   cityName  NCHAR (3) NOT NULL
);

CREATE TABLE Areas (
   addAreaid INT NOT NULL PRIMARY KEY IDENTITY (1, 1),
   addCityid INT NULL,
   areaName NVARCHAR (5) NULL
);

CREATE TABLE Userinfo (
   userid INT NOT NULL PRIMARY KEY IDENTITY(1,1),
   googleid VARCHAR(30),-- UNIQUE,
   uAccountEmail NVARCHAR(30) NOT NULL UNIQUE,
   uPwd NVARCHAR(60),
   uName NVARCHAR(25) NOT NULL,
   uNickname NVARCHAR(10),
   uGender CHAR(1),
   uBirthday DATE,
   uTel NVARCHAR(25) NOT NULL,
   addCityid INT,
   addAreaid INT,
   uidCode CHAR(10),
   uImgpath NVARCHAR (255),
   wrid INT NOT NULL ,
   FOREIGN KEY (addCityid) REFERENCES Citys(addCityid),
   FOREIGN KEY (addAreaid) REFERENCES Areas(addAreaid),
   FOREIGN KEY (wrid) REFERENCES WebRole(wrid),

);

----------------------------------------------------------------------


CREATE TABLE Organizer(
  oid int not null primary key identity(1,1),
  oName nvarchar(70) not null,
  oIntroduction nvarchar(1000) not null,
  oTel nvarchar(20),
  oPhone nvarchar(20),
  oEmail nvarchar(50) not null,
  oPicture nvarchar(255),
  oPictureType nvarchar(20),
  oHeadshot nvarchar(255),
  oHeadshotType nvarchar(20),
  oLinkA nvarchar(200),
  oLinkB nvarchar(200),
  oLinkC nvarchar(200),
  userid int not null,
  oIdentity nvarchar(20) not null,
  oIdcardFront nvarchar(255),
  oIdcardBack nvarchar(255),
  oAccPicture nvarchar(255),
  oCompanyId nvarchar(255),
  oTaxId char(8),
  oAccNumber nvarchar(50),
  FOREIGN KEY (userid) REFERENCES Userinfo(userid),
);


CREATE TABLE ResponseForUser(
   rfuid int not null primary key identity(1,1),
   rfuName nvarchar(20) not null,
   rfuEmail nvarchar(50) not null,
   rfuSubject nvarchar(60) not null,
   rfuCategory int not null,
   rfuContent nvarchar(1000) not null,
   rfuAskDate datetime not null,
   userid int,
   rfuResponseDate datetime ,
   rfuResponseName nvarchar(20),
   rfuStatus int not null,
    FOREIGN KEY (userid) REFERENCES Userinfo(userid)
);







CREATE TABLE Notification(
   ntfid int not null primary key identity(1,1),
   userid int not null,
   ntfTime datetime not null,
   ntfType nvarchar(20),
   ntfContent nvarchar(Max) not null,
   ntfReadStatus int not null,
   FOREIGN KEY (userid) REFERENCES Userinfo(userid)
);


---------------------------------------------------------------------------
CREATE TABLE Activity(
   acid int not null primary key identity(1,1),
   acName nvarchar(100) null,
   acIntro nvarchar(max) null,
   acSummary nvarchar(500) null,
   acSignUpStartDate datetime null,
   acSignUpEndDate datetime null,
   acStartDate datetime  null,
   acEndDate datetime  null,
   acCostStatus int  null,
   acCity nvarchar(10) null ,
   acArea nvarchar(10) null ,
   acAddress nvarchar(1000) null,
   acAddNote nvarchar(1000) null,
   acOnline nvarchar(2000) null,
   oid int not null ,
   acTel nvarchar(20) null,
   acPhone nvarchar(20) null,
   acEmail nvarchar(100) null,
   acCheckStatus int not null,
   acImg nvarchar(255) null,
   acImgCompress nvarchar(255) null,
   acLinkName nvarchar(150) null,
   acLinkUrl nvarchar(2000) null,
   acPublicStatus int null,
   acViewsCount int null,
   acRefundRules nvarchar(200) null,
   acSafeStatus int null,
   acPlaceStatus int null,
   acPreviousCheckStatus int null,
   acNotifyStatus int null,
   FOREIGN KEY (oid) REFERENCES Organizer(oid)
)



CREATE TABLE AllType(
   alltypeid int not null primary key identity(1,1),
   typeName nvarchar(20) not null
)



CREATE TABLE ActivityType(
   actypeid int not null primary key identity(1,1),
   acid int not null ,
   alltypeid int not null ,
   FOREIGN KEY (acid) REFERENCES Activity(acid),
   FOREIGN KEY (alltypeid) REFERENCES AllType(alltypeid)
)



CREATE TABLE ActivityImg(
   imgid int not null primary key identity(1,1),
   acid int not null,
   imgName nvarchar(50) not null,
   imgType varchar(15) not null,
   imgSize nvarchar(20)  not null,
   img varbinary(max) not null,
   FOREIGN KEY (acid) REFERENCES Activity(acid)
)



CREATE TABLE Favorite(
   likeid int not null primary key identity(1,1),
   userid int not null ,
   acid int not null ,
   FOREIGN KEY (userid) REFERENCES Userinfo(userid),
   FOREIGN KEY (acid) REFERENCES Activity(acid)
)


CREATE TABLE ActivityGuest(
guestid int not null primary key identity(1,1),
	acid int not null,
	guestName nvarchar(30) not null,
	guestJobTitle nvarchar(30) null,
	guestCompany nvarchar(50) null,
	guestIntro nvarchar(300) null,
	guestLink nvarchar(2000) null,
	guestImage nvarchar(255) null
	FOREIGN KEY (acid) REFERENCES Activity(acid)
)




----------------------------------------------------------
CREATE TABLE ViolationRecord(

   vrReportid INT PRIMARY KEY IDENTITY(1,1), --檢舉編號
   vrReportdatetime DATETIME NOT NULL, --檢舉日期
   vrViolationtype NVARCHAR(1000) NOT NULL, --違規事由
   vrReportuserid INT,  -- 檢舉人ID
   vrReporteduserid INT,  -- 被檢舉人ID
   vrReportedacid INT,  -- 活動ID
   vrReportedorgid INT,  -- 組織ID
   vrStatus INT,  -- 處理狀態
   artid INT,
   FOREIGN KEY (vrReportuserid) REFERENCES Userinfo(userid),
   FOREIGN KEY (vrReporteduserid) REFERENCES Userinfo(userid),
   FOREIGN KEY (vrReportedorgid) REFERENCES Organizer(oid),
   FOREIGN KEY (vrReportedacid) REFERENCES Activity(acid)
);

--------------------------------------------------------------------------

/*Follower 使用者追蹤主辦單位表*/

CREATE TABLE Follower (
   foid INT IDENTITY(1,1) PRIMARY KEY,
   userid INT not null,
   oid INT not null,
   FOREIGN KEY (userid) REFERENCES Userinfo(userid),
   FOREIGN KEY (oid) REFERENCES Organizer(oid)
);

/*ActivityComments 活動評論表*/

CREATE TABLE ActivityComments (
   commentid int PRIMARY KEY IDENTITY(1,1),
   commentTime datetime not null,
   commentContent nvarchar(1000) not null,
   commentScore int not null,
   userid int not null,
   acid int not null,
   CONSTRAINT FK_ActivityComments_Userinfo FOREIGN KEY (userid) REFERENCES Userinfo(userid),
   CONSTRAINT FK_ActivityComments_Activity FOREIGN KEY (acid) REFERENCES Activity(acid)
);



/*ActivityTickets 活動票券表*/

CREATE TABLE [dbo].[ActivityTickets] (
 [atid]         INT    PRIMARY KEY  IDENTITY (1, 1) NOT NULL,
 [acid]          INT           NOT NULL,
 [atName]       NVARCHAR (50) NOT NULL,
 [atIntro] nvarchar(400)           NOT NULL,
 [atImg]        nvarchar(255) ,
 [atImgType]        nvarchar(5)          ,
 [atImgName]       nvarchar(30)      ,
 [atImgWidth] INT           ,
 [atPrice]   INT      NOT NULL,
 [atQuantity]   INT      NOT NULL,
 [atDirection] nvarchar(400)           NOT NULL,
 [atIsAlternate] TINYINT           NOT NULL,
 FOREIGN KEY ([acid]) REFERENCES [dbo].[Activity] ([acid])
);





/*PayMethod*/
CREATE Table PayMethod(
pmid int not null primary key,
pmName NVARCHAR(10)
);


/*AcPayMethod*/
CREATE Table AcPayMethod (
  apmid INT IDENTITY(1,1) PRIMARY KEY,
  acid INT,
  pmid INT,
  FOREIGN KEY (acid) REFERENCES Activity(acid),
  FOREIGN KEY (pmid) REFERENCES PayMethod(pmid)
);

/*3/19 新增atid, pmid*/
CREATE TABLE AcSignForm(
   asfid int PRIMARY KEY IDENTITY(1,1),
   acid int  NOT NULL references Activity(acid),
   asfName nvarchar(25)  NOT NULL,
asfEmail nvarchar(30) NOT NULL,
   asfTel nvarchar(25)  NOT NULL,
   asfGender char(1) NOT NULL,
   asfBirthday date NOT NULL,
   asfCity nvarchar(10) NOT NULL,
   asfArea nvarchar(10) NOT NULL ,
   userid int references Userinfo(userid),
   asfDate datetime  NOT NULL,
   asfPaidStatus int ,
atid int NOT NULL,
pmid int NOT NULL,
   asfPaidDate datetime ,
   asfSignStatus int  NOT NULL,
    transactionId  VARCHAR (50)  NULL,
    orderId      VARCHAR (50)  NULL,
    cancelTime    DATETIME      NULL,
asfHash char(64) null,
asfVerified char(1) null,
asfQRcode uniqueidentifier null,

   FOREIGN KEY (acid) REFERENCES Activity(acid),
   FOREIGN KEY (userid) REFERENCES Userinfo(userid),
   FOREIGN KEY (atid) REFERENCES ActivityTickets(atid),
 
)






CREATE TABLE Article (
        artid INT PRIMARY KEY IDENTITY(1,1),
    artTitle NVARCHAR(100) NOT NULL,
    artContent NVARCHAR(MAX) NOT NULL,
    artCreateTime DATETIME NOT NULL,
    artLastEditTime DATETIME NOT NULL,
    userid INT NOT NULL,
    acid INT,
        artIsOther TINYINT NOT NULL,
    artViewCount INT,
        artStatus TINYINT,
    FOREIGN KEY (userid) REFERENCES Userinfo(userid),
    FOREIGN KEY (acid) REFERENCES Activity(acid)
);


CREATE TABLE ArticleLikes (
   alid INT PRIMARY KEY IDENTITY(1,1),
   userid INT NOT NULL,
   artid INT NOT NULL,
   alIsLike TINYINT,
   FOREIGN KEY (userid) REFERENCES Userinfo(userid),
   FOREIGN KEY (artid) REFERENCES Article(artid)
);

CREATE TABLE ArticleComments (
   artcid INT PRIMARY KEY IDENTITY(1,1),
   artid INT NOT NULL,
   artcContent NVARCHAR(MAX) NOT NULL,
   artcCreateTime DATETIME NOT NULL,
   userid INT NOT NULL,
   FOREIGN KEY (userid) REFERENCES Userinfo(userid),
   FOREIGN KEY (artid) REFERENCES Article(artid)
);


/*新增會員興趣表*/
CREATE TABLE UserInterest(
   uitid int not null primary key identity(1,1),
   userid int not null ,
   alltypeid int not null ,
   FOREIGN KEY (userid) REFERENCES Userinfo(userid),
   FOREIGN KEY (alltypeid) REFERENCES AllType(alltypeid)
)


/*忘記密碼使用的TOKEN驗證*/
CREATE TABLE PasswordResetToken(
pwdTokenid INT not null PRIMARY KEY IDENTITY(1,1),
userid INT NOT NULL,
token Char(36) NOT NULL,
expiryDate DATETIME NOT NULL,
FOREIGN KEY (userid) REFERENCES Userinfo(userid)
)


USE [jojo]
GO

/****** Object:  Trigger [dbo].[trgBeforeUpdateOnActivity]    Script Date: 2024/3/24 下午 03:29:11 ******/
SET ANSI_NULLS ON
GO

SET QUOTED_IDENTIFIER ON
GO


CREATE TRIGGER [dbo].[trgBeforeUpdateOnActivity]
ON [dbo].[Activity]
INSTEAD OF UPDATE
AS
BEGIN
    SET NOCOUNT ON;

    -- 更新Activity表的字段
    UPDATE a
    SET
        a.acName = i.acName,
        a.acIntro = i.acIntro,
        a.acSummary = i.acSummary,
        a.acSignUpStartDate = i.acSignUpStartDate,
        a.acSignUpEndDate = i.acSignUpEndDate,
        a.acStartDate = i.acStartDate,
        a.acEndDate = i.acEndDate,
        a.acCostStatus = i.acCostStatus,
        a.acCity = i.acCity,
        a.acArea = i.acArea,
        a.acAddress = i.acAddress,
        a.acAddNote = i.acAddNote,
        a.acOnline = i.acOnline,
        a.oid = i.oid,
        a.acTel = i.acTel,
        a.acPhone = i.acPhone,
        a.acEmail = i.acEmail,
        a.acCheckStatus = i.acCheckStatus,
        a.acImg = i.acImg,
        a.acImgCompress = i.acImgCompress,
        a.acLinkName = i.acLinkName,
        a.acLinkUrl = i.acLinkUrl,
        a.acPublicStatus = i.acPublicStatus,
        a.acViewsCount = i.acViewsCount,
        a.acRefundRules = i.acRefundRules,
        a.acSafeStatus = i.acSafeStatus,
        a.acPlaceStatus = i.acPlaceStatus,
        a.acNotifyStatus = i.acNotifyStatus,
        a.acPreviousCheckStatus = CASE 
            WHEN i.acCheckStatus = 2 AND d.acCheckStatus = 1 THEN d.acCheckStatus 
            ELSE a.acPreviousCheckStatus 
        END
    FROM [dbo].[Activity] a
    INNER JOIN inserted i ON a.acid = i.acid
    INNER JOIN deleted d ON a.acid = d.acid;
END
GO

ALTER TABLE [dbo].[Activity] ENABLE TRIGGER [trgBeforeUpdateOnActivity]
GO



USE [jojo]
GO

/****** Object:  Trigger [dbo].[DeleteArticleOnViolation]    Script Date: 2024/4/4 上午 09:58:06 ******/
SET ANSI_NULLS ON
GO

SET QUOTED_IDENTIFIER ON
GO

CREATE TRIGGER [dbo].[DeleteArticleOnViolation]
ON [dbo].[ViolationRecord]
AFTER UPDATE
AS
BEGIN
    SET NOCOUNT ON;

    IF UPDATE(vrStatus)
    BEGIN
        DECLARE @artid INT;
        
        SELECT @artid = INSERTED.artid
        FROM INSERTED
        WHERE INSERTED.vrStatus = 2 AND INSERTED.artid IS NOT NULL;

        IF @artid IS NOT NULL
        BEGIN
            DELETE FROM Article WHERE artid = @artid;
        END
    END
END;
GO

ALTER TABLE [dbo].[ViolationRecord] ENABLE TRIGGER [DeleteArticleOnViolation]
GO

